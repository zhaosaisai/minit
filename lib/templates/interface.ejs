<% interfaces.forEach(_interface => { %>
<% if (_interface.description) { %>
/**
*
* description: <%- _interface.description %>
*
*/
<% } %>
interface <%- _interface.name %> {
<% _interface.fields.forEach(field => { %>
	<%- field.name %><%- field.required ? ':' : '?:' %> <%- field.type %>
<% }) %>
}
<% }) %>
