import axios from 'axios'

<% requests.forEach(request => { %>
<% if (request.description) { %>
/**
*
* description: <%- request.description %>
*
*/
<% } %>
const <%- request.name %> = (<% if (request.bodyParams) { %>data: <%- request.bodyParams %><% } %><%- request.bodyParams&&request.queryParams&&', ' %><% if (request.queryParams) { %>params: <%- request.queryParams %><% } %>): Promise<<%- request.responseSchema %>> => axios.<%- request.method %>('<%- request.url %>', <%- request.bodyParams ? 'data, ' : '' %><%- request.queryParams ? '{ params }' : '{}' %>)
<% }) %>
